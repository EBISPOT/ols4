{
  "name": "ols4-widgets",
  "version": "4.0.0",
  "main": "dist/entity-tree-esm/index.js",
  "module": "dist/entity-tree-esm/index.js",
  "proxy": "http://www.ebi.ac.uk/ols4/",
  "private": true,
  "types": "dist/entity-tree-esm/index.d.ts",
  "dependencies": {
  },
  "scripts": {
    "start": "concurrently -n 'React,Tailwind' 'react-scripts start' 'yarn watch:css'",
    "watch:css": "tailwindcss -i ./src/index.css -o ./public/main.css --minify --watch",
    "build": "yarn build:css && bash -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "et_clear": "rimraf dist/entity-tree-esm/*",
    "et_make-bundle": "rollup --config",
    "et_cp_package-json": "cp package.json dist/entity-tree-esm/package.json",
    "et_cp_styles": "cp -r src/widgets/styles/ dist/entity-tree-esm/widgets/styles/",
    "build-et": "yarn run et_clear && yarn run tsc && yarn run et_make-bundle && yarn et_cp_package-json && yarn et_cp_styles",
    "build:css": "tailwindcss -i ./src/index.css -o ./public/main.css --minify",
    "build:sandbox": "REACT_APP_ENV=sandbox npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "build:docker": "REACT_APP_ENV=docker npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.2.3",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^8.3.4",
    "rollup": "^2.77.2",
    "tailwindcss": "^3.1.8"
  }
}
