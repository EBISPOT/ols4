{
  "name": "ols",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/styles": "^5.9.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/multimap": "^1.1.2",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/three": "^0.152.1",
    "concurrently": "^7.5.0",
    "country-code-to-flag-emoji": "^1.2.1",
    "date-fns": "^2.19.0",
    "file-saver": "^2.0.5",
    "history": "^5.0.0",
    "immutable": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "multimap": "^1.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.8.0",
    "react-twitter-widgets": "^1.11.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "build": "dotenv -e .env.$REACT_APP_ENV -e .env -- node build.mjs",
    "watch": "dotenv -e .env.$REACT_APP_ENV -e .env -- nodemon -e 'ts,tsx,js,jsx,css' -w src -w index.html.in --exec 'npm run build'",
    "start": "dotenv -e .env.$REACT_APP_ENV -e .env -- concurrently -n 'Build,Serve' 'npm run watch' 'http-server dist -p 3000 --proxy $OLS_DEV_BACKEND_PROXY_URL'",
    "build:prod": "OLS_ENV=prod npm run build",
    "et_clear": "rimraf dist/entity-tree-esm/*",
    "et_make-bundle": "rollup --config",
    "et_cp_package-json": "cp package.json dist/entity-tree-esm/package.json",
    "et_cp_styles": "cp -r src/widgets/styles/ dist/entity-tree-esm/widgets/styles/",
    "build-et": "yarn run et_clear && yarn run tsc && yarn run et_make-bundle && yarn et_cp_package-json && yarn et_cp_styles",
    "build:css": "tailwindcss -i ./src/index.css -o ./public/main.css --minify"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.18.11",
    "http-server": "^14.1.1",
    "nodemon": "^2.0.22",
    "tailwindcss": "^3.1.8"
  }
}
